  <body>
    <div class="wrapper row g-0">
      <div class="col-2 d-flex flex-column py-5 left-drawer">
        <a class="logo px-5" href="javascript:;">
          <img src="./assets/images/logo-ANSWER.svg" alt="logo" />
        </a>
        <ul class="drawers list-unstyled pt-3">
          <li class="drawer px-5 my-4">
            <a
              href="javascript:;"
              class="d-flex align-items-center fw-bold py-1 hover-change-main-color"
            >
              <span class="material-symbols-outlined me-3">insert_chart</span>
              Dashboard
            </a>
          </li>
          <li class="drawer px-5 my-4">
            <a
              href="javascript:;"
              class="d-flex align-items-center fw-bold py-1 hover-change-main-color"
            >
              <span class="material-symbols-outlined me-3">forum</span>
              Question
            </a>
          </li>
          <li class="drawer px-5 my-4">
            <a
              href="javascript:;"
              class="d-flex align-items-center fw-bold py-1 hover-change-main-color"
            >
              <span class="material-symbols-outlined me-3">assignment</span>
              Assignment
            </a>
          </li>
          <li class="drawer px-5 my-4">
            <a
              href="javascript:;"
              class="d-flex align-items-center fw-bold py-1 hover-change-main-color"
            >
              <span class="material-symbols-outlined me-3">video_library</span>
              Course
            </a>
          </li>
          <li class="drawer px-5 my-4 active">
            <a
              href="javascript:;"
              class="d-flex align-items-center fw-bold py-1 hover-change-main-color"
            >
              <span class="material-symbols-outlined me-3">group</span>
              Admin
            </a>
          </li>
          <li class="drawer px-5 my-4">
            <a
              href="javascript:;"
              class="d-flex align-items-center fw-bold py-1 hover-change-main-color"
            >
              <span class="material-symbols-outlined me-3">settings</span>
              Settings
            </a>
          </li>
        </ul>
        <a
          href="javascript:;"
          class="user row g-0 align-items-center flex-nowrap hover-change-main-color"
        >
          <div class="col-auto avatar me-3">
            <img src="./assets/images/andychen.jpeg" alt="andychen" />
          </div>
          <div class="col detail column">
            <div class="user-name fw-bold">Andy Chen</div>
            <div class="user-account text-gray-600">andy0401@mail.com</div>
          </div>
        </a>
      </div>
      <div class="col"><%- contents %></div>
    </div>
